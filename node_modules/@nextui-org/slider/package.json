    "@react-aria/visually-hidden": "3.8.12",
    "@react-stately/slider": "3.5.4",
    "@nextui-org/tooltip": "2.0.39",
    "@nextui-org/shared-utils": "2.0.7",
    "@nextui-org/react-utils": "2.0.16"
  },
  "clean-package": "../../../clean-package.config.json",
